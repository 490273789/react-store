{
  "name": "react-store",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish",
    "test": "jest --passWithNoTests --config jest.config.ts",
    "eslint": "eslint --no-eslintrc -c .eslint.config.js --fix '**/src/*.{js,jsx,ts,tsx}'",
    "eslint:ci": "eslint --no-eslintrc -c .eslint.config.js '**/src/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier '**/{examples,src,__tests__,website}/**/*.{js,jsx,ts,tsx,md}' --write",
    "prettier:ci": "prettier '**/{examples,src,__tests__,website}/**/*.{js,jsx,ts,tsx,md}' --list-different",
    "typecheck": "pnpm -r --parallel run typecheck",
    "build": "concurrently 'pnpm:build:*'",
    "build:jotai": "rollup -c --package jotai",
    "build:zustand": "rollup -c --package zustand",
    "build:valtio": "rollup -c --package valtio",
    "build:react-query": "rollup -c --package react-query",
    "type-check:ci": "pnpm -r --parallel run typecheck",
    "format": "prettier --write",
    "lint": "eslint .",
    "prepare": "husky",
    "common": "pnpm --filter common"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-react-jsx": "^7.25.9",
